{
  "name": "site",
  "version": "0.7.0",
  "private": true,
  "dependencies": {
    "mdx-loader": "^1.0.0-beta.3",
    "mdxc": "^1.0.0",
    "react": "^16.5.0",
    "react-dom": "^16.5.0",
    "react-scripts": "^1.1.5",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "clean": "rimraf build",
    "start": "react-scripts start",
    "build": "react-scripts build && navi build -e build/static/js/main.*.js",
    "map": "react-scripts build && navi map -e build/static/js/main.*.js -j -o build/static/js/map.json",
    "test": "react-scripts test --env=jsdom",
    "serve": "navi serve -p 3000"
  }
}
