{
  "name": "junctions-static",
  "engine": {
    "node": ">=8.0.0"
  },
  "version": "0.4.0-13",
  "description": "Build static websites using Junctions.",
  "author": "James K Nelson <james@jamesknelson.com>",
  "license": "MIT",
  "bin": {
    "junctions-static": "./bin/junctions-static.js"
  },
  "main": "lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:watch": "cross-env BABEL_ENV=commonjs babel --watch --source-maps -d lib/ src/",
    "prepublish": "npm run clean && npm run build"
  },
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-register": "^6.24.0",
    "chalk": "^2.3.0",
    "commander": "^2.9.0",
    "escape-string-regexp": "^1.0.5",
    "finalhandler": "^1.1.0",
    "fs-extra": "^5.0.0",
    "jsdom": "^11.5.1",
    "serve-static": "^1.11.2"
  },
  "devDependencies": {
    "cross-env": "^5.0.5",
    "rimraf": "^2.5.4"
  },
  "keywords": [
    "routing",
    "junctions",
    "static"
  ]
}
